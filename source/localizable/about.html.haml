.about_me
  .row
    .small-12.medium-6.medium-offset-3.large-8.large-offset-2.columns
      %h1= t(:about_title)
      %p= t(:about_description)
      #skills
        %p= t(:about_skills_title)
        %ul
        - t(:about_skills_list).each do |skill|
          %li= skill
        %p
        = t(:about_skills_end)
